{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@narendev/cyanite-sdk/(.*)": "<rootDir>/../libs/cyanite-sdk/src/$1",
    "@narendev/cyanite-sdk": "<rootDir>/../libs/cyanite-sdk/src",
    "@narendev/apple-music-sdk/(.*)": "<rootDir>/../libs/apple-music-sdk/src/$1",
    "@narendev/apple-music-sdk": "<rootDir>/../libs/apple-music-sdk/src",
    "@narendev/spotify-sdk/(.*)": "<rootDir>/../libs/spotify-sdk/src/$1",
    "@narendev/spotify-sdk": "<rootDir>/../libs/spotify-sdk/src"
  }
}