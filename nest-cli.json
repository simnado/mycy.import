{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "cyanite-sdk": {
      "type": "library",
      "root": "libs/cyanite-sdk",
      "entryFile": "index",
      "sourceRoot": "libs/cyanite-sdk/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cyanite-sdk/tsconfig.lib.json"
      }
    }
  }
}